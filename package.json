{
  "name": "pocketdex",
  "version": "0.0.1",
  "description": "Pocketdex is an indexer for the Shannon implementation of the pocket network protocol.",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "postinstall": "yarn run env:prepare && POSTINSTALL=\"true\" yarn run vendors:build",
    "precommit": "./scripts/before-commit.sh",
    "clean:all": "./scripts/remove-all-node-modules.sh",
    "codegen": "yarn exec subql codegen",
    "build": "./scripts/build.sh",
    "vendors:build": "node vendor-builder.js",
    "vendors:reset": "./scripts/update-submodules.sh && yarn run vendors:build",
    "watch:build": "./scripts/build.sh no-lint",
    "lint": "yarn exec eslint src --ext .ts",
    "lint:fix": "yarn exec eslint src --ext .ts --fix",
    "format": "yarn exec prettier --loglevel error --write 'src/*.{js,ts,jsx,tsx}'",
    "format:ci": "yarn exec pretty-quick --verbose --check --pattern 'src/*.{js,ts,jsx,tsx}' --branch origin/main",
    "test": "yarn run build && subql-node-cosmos test",
    "test:ci": "env node vendor/subql-cosmos/packages/node/bin/run test",
    "env:prepare": "./scripts/prepare-dotenv.sh",
    "docker:compose": "./scripts/docker-compose.sh",
    "poktroll:proxy:start": "./scripts/proxy-tunnel.sh start",
    "poktroll:proxy:stop": "./scripts/proxy-tunnel.sh stop",
    "poktroll:update-proto-files": "scripts/copy-poktroll-files.sh",
    "docker:check-env:production": "env sh ./scripts/dotenv-check.sh production",
    "docker:build:production": "yarn run docker:compose production build",
    "docker:build:no-cache:production": "yarn run docker:compose production build --no-cache",
    "docker:start:production": "yarn run docker:compose production up --remove-orphans",
    "docker:ps:production": "yarn run docker:compose production ps",
    "docker:stop:production": "yarn run docker:compose production stop",
    "docker:clean:production": "yarn run docker:compose production down -v",
    "docker:check-env:development": "env sh ./scripts/dotenv-check.sh development",
    "docker:build:development": "yarn run docker:compose development build",
    "docker:build:no-cache:development": "yarn run docker:compose development build --no-cache",
    "docker:start:development": "yarn run docker:compose development up --remove-orphans",
    "docker:ps:development": "yarn run docker:compose development ps",
    "docker:stop:development": "yarn run docker:compose development stop",
    "docker:clean:development": "yarn run docker:compose development down -v",
    "docker:check-env:test": "env sh ./scripts/dotenv-check.sh test",
    "docker:build:test": "yarn run docker:compose test build",
    "docker:build:no-cache:test": "yarn run docker:compose test build --no-cache",
    "docker:start:test": "yarn run docker:compose test up --remove-orphans",
    "docker:ps:test": "yarn run docker:compose test ps",
    "docker:stop:test": "yarn run docker:compose test stop",
    "docker:clean:test": "yarn run docker:compose test down -v"
  },
  "homepage": "https://github.com/pokt-network/pocketdex",
  "repository": "github:pokt-network/pocketdex",
  "packageManager": "yarn@3.8.0",
  "engines": {
    "node": "18.x"
  },
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "POKTscan & Grove Team",
  "license": "MIT",
  "devDependencies": {
    "@subql/cli": "5.5.1",
    "@subql/testing": "2.2.3",
    "@types/stream-json": "^1.7.8",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "js-yaml": "4.1.0",
    "nodemon": "3.1.4",
    "prettier": "2.5.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.3",
    "@subql/node-core": "16.2.2-0",
    "@subql/node-cosmos": "4.5.1",
    "@subql/types-cosmos": "4.1.0",
    "@subql/utils": "2.17.2",
    "@types/json-bigint": "1.0.4",
    "dotenv": "16.4.7",
    "json-bigint": "1.0.0",
    "lodash": "4.17.21",
    "node-fetch": "^3.3.2",
    "starknet": "6.11.0",
    "stream-json": "^1.9.1",
    "ts-proto": "^1.112.1",
    "tslib": "^2.3.1"
  }
}
