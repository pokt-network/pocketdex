# Syncs changes in .gitmodules
# git submodule sync
# git submodule update --init --recursive --remote

[submodule "vendor/subql"]
	path = vendor/subql
	url = https://github.com/pokt-scan/subql.git
	branch = feature/expose-orm
[submodule "vendor/subql-cosmos"]
	path = vendor/subql-cosmos
	url = https://github.com/pokt-scan/subql-cosmos.git
	branch = main
[submodule "vendor/cosmjs"]
	path = vendor/cosmjs
	url = https://github.com/pokt-scan/x-cosmjs
	branch = feature/add-module-accounts
